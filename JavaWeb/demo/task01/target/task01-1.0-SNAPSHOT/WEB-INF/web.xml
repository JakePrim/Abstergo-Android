    <!-- 进行Servlet映射配置 -->
    <servlet-mapping>
        <servlet-name>TestServlet2</servlet-name>
        <!-- 配置浏览器的一个访问地址 -->
        <url-pattern>/test2</url-pattern>
    </servlet-mapping>


    <!-- 配置Servlet -->
    <servlet>
        <!-- 对Servlet起别名 -->
        <servlet-name>TestServlet</servlet-name>
        <!-- 对应的Servlet类 -->
        <servlet-class>com.jakeprim.task01.TestServlet</servlet-class>
    </servlet>
    <!-- 进行Servlet映射配置 -->
    <servlet-mapping>
