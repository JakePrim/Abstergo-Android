<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.prim.order.mapper.OrderItemMapper">

    <select id="findByOrderId" resultType="com.prim.common.pojo.OrderItem">
        select *
        from order_item
        where order_id = #{id}
    </select>
</mapper>